<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/vendorinkitchen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        >

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/vendorimage"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/blackimage"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            />



    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/vendorinKitchenDetails"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_toEndOf="@id/vendorinkitchen"
        android:background="@drawable/bg_border_vk"
        >



    </RelativeLayout>



</RelativeLayout>